{% extends "layout.twig" %}

{% block content %}
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
        {{ block(contentBlock) }}
    </div>
{% endblock %}

{% block noRecord %}
    <h1 class="page-header">{{ message }}</h1>

    <form action="{{ url('repository_createDump', {'repositorySlug': repositorySlug}) }}" method="post">
        <button value="create">Create a new Record</button>
    </form>
{% endblock %}